public class first
{
	public static void main(String args[])
{
	int no2,no2,ans;

	no1 = 11;
	no2 = 3;

	ans = no1 + no2;
	System.out.println(ans);

	ans = no1 - no2;
	System.out.println(ans);

	ans = no1 * no2;
	System.out.println(ans);

	
	double no1, no2;
	ans = no1 / no2;
	System.out.println(ans);
}
}
